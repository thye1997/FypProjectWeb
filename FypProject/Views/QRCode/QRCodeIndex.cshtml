

<div class="alert hide-btn" id="QRCode-list-alert-message" role="alert">
    <button type="button" class="close" id="close-alert" onclick="closeMessageFunc($('#QRCode-list-alert-message'))">
        <span>&times;</span>
    </button>
</div>
<div class="float-right m-3">
    <!--<form method="post" asp-controller="QRCode" asp-action="GenerateQRCode">-->
        <button class="btn btn-outline-primary btn-sm" style="font-size:16px;" onclick="GenerateQRCode()">
            Generate QR Code
        </button>
   <!-- </form>-->
</div>

<div class="text-center">
    <div class="table-responsive p-3">
        <table class="table table-bordered dt-responsive" width="100%" id="QRCode-list-table">
            <thead>
                <tr>
                    <th>File Name</th>
                    <th>Created By</th>
                    <th>Created On</th>
                    <th>Status</th>
                    <th>Action</th>
                </tr>
            </thead>
        </table>
    </div>
</div>


@section Scripts {
    <script type="text/javascript" src="~/js/QRCode/qrcode.js"></script>
}